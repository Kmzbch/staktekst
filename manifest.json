{
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Staktekst",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "lib/jquery-3.5.0.min.js",
        "newTab.js"
      ],
      "matches": [
        "http://www.oddcast.com/ttsdemo/index.php",
        "https://miraitranslate.com/trial/",
        "https://miraitranslate.com/en/trial/"
      ],
      "run_at": "document_end"
    },
    {
      "js": [
        "lib/jquery-3.5.0.min.js",
        "bubble_lib.js",
        "currentTab.js"
      ],
      "css": [
        "currentTab.css",
        "css/brands.min.css",
        "css/solid.min.css",
        "css/font-awesome.min.css",
        "css/material-icons.css"
      ],
      "matches": [
        "\u003Call_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "images/*",
    "webfonts/*",
    "icons/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' chrome-extension-resource: ; object-src 'self' ; child-src 'self' ; style-src 'self' ; frame-src 'self' ; default-src 'self' ; img-src 'self' ; connect-src 'self' ",
  "description": "Paragraph-based learning aid for ESL learners",
  "icons": {
    "16": "icons/icon_16.png",
    "19": "icons/icon_19.png",
    "38": "icons/icon_38.png",
    "128": "icons/icon_128.png"
  },
  "manifest_version": 2,
  "name": "Staktekst",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "unlimitedStorage",
    "contextMenus",
    "downloads"
  ],
  "version": "0.1.0"
}